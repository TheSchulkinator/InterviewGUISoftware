<Window x:Class="InterviewGUISoftware.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InterviewGUISoftware"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid x:Name="MinMaxAverageDataGrid">
        <DataGrid  HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="497" Height="130" />
        <TextBox x:Name="tMinTextBox" HorizontalAlignment="Left" Height="23" Margin="10,177,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="tMaxTextBox" HorizontalAlignment="Left" Height="23" Margin="162,177,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="SubmitTime" Content="Submit Time" HorizontalAlignment="Left" Margin="317,180,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBlock x:Name="tMinHint" HorizontalAlignment="Left" Margin="10,156,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
        <TextBlock x:Name="tMaxHint" HorizontalAlignment="Left" Margin="162,156,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>

    </Grid>
</Window>
